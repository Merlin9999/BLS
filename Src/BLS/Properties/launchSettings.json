{
  "profiles": {
    "BLS - No Args": {
      "commandName": "Project"
    },
    "BLS - All Files": {
      "commandName": "Project",
      "commandLineArgs": "**\\*"
    },
    "BLS - Project Files - All": {
      "commandName": "Project",
      "commandLineArgs": "..\\..\\..\\**\\*"
    },
    "BLS - ProjectFiles - Exclude bin & obj": {
      "commandName": "Project",
      "commandLineArgs": "**\\* -b ..\\..\\.. -x bin\\**\\* obj\\**\\*"
    },
    "BLS - ProjectFiles & Parent .vs": {
      "commandName": "Project",
      "commandLineArgs": "**\\* -b ..\\..\\.. ..\\..\\..\\.. -x bin\\**\\* obj\\**\\* LS\\**"
    },
    "BLS - ProjectFiles & Parent & Sorted": {
      "commandName": "Project",
      "commandLineArgs": "**\\* -b ..\\..\\.. ..\\..\\..\\.. -x **\\bin\\**\\* **\\obj\\**\\* -s"
    },
    "BLS - Path Executables": {
      "commandName": "Project",
      "commandLineArgs": "path *.exe *.com *.bat *.con *.cmd"
    },
    "BLS - Users - Exceptions": {
      "commandName": "Project",
      "commandLineArgs": "**\\* -b c:\\Users"
    },
    "BLS - C:\\": {
      "commandName": "Project",
      "commandLineArgs": "* -b c:\\"
    },
    "BLS - zip": {
      "commandName": "Project",
      "commandLineArgs": "zip **\\* -x **\\bin\\** **\\obj\\** -b ..\\..\\.. ..\\..\\..\\.. -z Test.zip"
    }
  }
}